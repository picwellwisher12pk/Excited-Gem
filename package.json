{
  "name": "excited-gem",
  "displayName": "Excited Gem",
  "version": "1.3.0",
  "description": "A tool to manage your tabs and other browser related features.",
  "homepage": "https://picwellwisher12pk.github.io/excited-gem/",
  "license": "MIT",
  "author": "Amir Hameed",
  "client": {
    "chrome": "chrome",
    "firefox": "browser"
  },
  "browser": {
    "./dist/index.js": "./firefox/tabs.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:picwellwisher12pk/Excited-Gem.git"
  },
  "scripts": {
    "start": "nodemon",
    "dev": "plasmo dev",
    "dev:win": "plasmo dev & ./scripts/sync-build.sh",
    "build": "plasmo build",
    "package": "plasmo package",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf extension/dist extension/manifest.json extension.*",
    "format": "bunx @biomejs/biome format ./ --write",
    "lint": "bunx @biomejs/biome lint ./ --apply",
    "react-devtools": "react-devtools",
    "depcheck": "npx depcheck",
    "outdated": "npm outdated",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ffflorian/jszip-cli": "^3.8.2",
    "@plasmohq/redux-persist": "6.1.0",
    "@plasmohq/storage": "^1.13.0",
    "@react-buddy/ide-toolbox": "^2.4.0",
    "@react-buddy/palette-antd": "^5.3.1",
    "@reduxjs/toolkit": "^2.5.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/react-window": "^2.0.0",
    "antd": "^5.24.3",
    "chokidar": "^4.0.3",
    "content-security-policy-parser": "^0.6.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.6",
    "cssnano": "^7.0.6",
    "dotenv": "^16.3.1",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "glob": "^13.0.0",
    "html-react-parser": "^5.2.1",
    "immutability-helper": "^3.1.1",
    "kolorist": "^1.8.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.554.0",
    "mocha": "10.2.0",
    "msgpackr": "^1.11.5",
    "plasmo": "0.90.5",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.0.0",
    "react": "^18.2.0",
    "react-content-loader": "^7.0.2",
    "react-custom-scroll": "^7.0.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-lazy-load-image-component": "^1.6.3",
    "react-redux": "^9.2.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.2.3",
    "redux": "^5.0.1",
    "redux-async-initial-state": "^0.3.0",
    "redux-logger": "^4.0.0",
    "redux-persist-webextension-storage": "1.0.2",
    "redux-thunk": "3.1.0",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@svgr/rollup": "^8.1.0",
    "@types/chrome": "0.0.308",
    "@types/node": "20.17.10",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@welldone-software/why-did-you-render": "^8.0.3",
    "autoprefixer": "^10.4.22",
    "clean-publish": "^4.2.0",
    "jest": "^29.6.1",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.6",
    "react-devtools": "^6.1.1",
    "tailwindcss": "3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "storage",
      "sidePanel",
      "tabGroups",
      "identity",
      "bookmarks",
      "unlimitedStorage"
    ],
    "side_panel": {
      "default_path": "tabs/home.html"
    },
    "host_permissions": [
      "https://*.youtube.com/*",
      "http://*.youtube.com/*"
    ],
    "oauth2": {
      "client_id": "895689043158-ilstmlvd16i9p88n22b7sva0d6mhnp6n.apps.googleusercontent.com",
      "scopes": [
        "openid",
        "email",
        "profile",
        "https://www.googleapis.com/auth/drive.appdata"
      ]
    }
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": 1000
  }
}