{
  "name": "excited-gem",
  "version": "0.7.3",
  "description": "A tabs management extension for Chrome",
  "homepage": "https://picwellwisher12pk.github.io/onetab-advanced/",
  "bugs": "{}",
  "license": "MIT",
  "author": "Amir Hameed",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/picwellwisher12pk/onetab-advanced.git"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "node scripts/build.js",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css"
  },
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "autoprefixer-stylus": "0.10.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-react-app": "^0.2.1",
    "bootstrap-loader": "^2.1.0",
    "bootstrap-sass": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "compression-webpack-plugin": "^1.0.0",
    "concurrently": "3.0.0",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.0",
    "css-loader": "^0.24.0",
    "detect-port": "1.0.0",
    "dotenv": "2.0.0",
    "eslint": "3.5.0",
    "eslint-config-react-app": "^0.2.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-flowtype": "2.18.1",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.22.0",
    "http-proxy-middleware": "0.17.1",
    "jest": "15.1.1",
    "json-loader": "0.5.4",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "0.13.0",
    "promise": "7.1.1",
    "pug": "latest",
    "pug-loader": "^2.3.0",
    "react-dev-utils": "^0.2.1",
    "recursive-readdir": "2.1.0",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "2.5.4",
    "sass-loader": "^6.0.6",
    "strip-ansi": "3.0.1",
    "style-loader": "^0.13.1",
    "stylus": "0.54.5",
    "url-loader": "^0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "whatwg-fetch": "1.0.0",
    "zip-webpack-plugin": "^2.0.0"
  },
  "dependencies": {
    "archiver": "2.0.0",
    "bootstrap-sass": "^3.3.7",
    "crx": "3.2.1",
    "crx-webpack-plugin": "0.1.5",
    "history": "4.2.0",
    "jquery": "latest",
    "re-base": "2.2.0",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "4.0.0-alpha.4"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jquery": true
    },
    "plugins": [
      "react"
    ],
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>\\config\\jest\\FileStub.js",
      "^.+\\.css$": "<rootDir>\\config\\jest\\CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>\\config\\polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
