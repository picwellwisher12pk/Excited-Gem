{
  "name": "excited-gem",
  "displayName ": "Excited Gem",
  "version": "1.0.0",
  "description": "A tool to manage your tabs and other browser related features.",
  "homepage": "https://picwellwisher12pk.github.io/onetab-advanced/",
  "license": "MIT",
  "author": "Amir Hameed",
  "client": {
    "chrome": "chrome",
    "firefox": "browser"
  },
  "browser": {
    "./dist/index.js": "./firefox/tabs.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:picwellwisher12pk/Excited-Gem.git"
  },
  "scripts": {
    "start": "nodemon",
    "dev": "plasmo dev",
    "build": "plasmo build",
    "postbuild": "git add . ",
    "package": "plasmo package",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf extension/dist extension/manifest.json extension.*",
    "format": "bunx @biomejs/biome format ./ --write",
    "lint": "bunx @biomejs/biome lint ./ --apply",
    "react-devtools": "react-devtools",
    "depcheck": "npx depcheck",
    "outdated": "npm outdated",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@ffflorian/jszip-cli": "^3.8.2",
    "@plasmohq/redux-persist": "6.1.0",
    "@plasmohq/storage": "^1.13.0",
    "@react-buddy/ide-toolbox": "^2.4.0",
    "@react-buddy/palette-antd": "^5.3.1",
    "@reduxjs/toolkit": "^2.5.0",
    "@tailwindcss/forms": "^0.5.10",

    "antd": "^5.24.3",
    "chokidar": "^4.0.3",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "dotenv": "^16.3.1",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "html-react-parser": "^5.2.1",
    "immutability-helper": "^3.1.1",
    "kolorist": "^1.8.0",
    "lodash": "^4.17.21",
    "mocha": "10.2.0",
    "plasmo": "0.90.3",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.0.0",
    "react": "^18.2.0",
    "react-content-loader": "^7.0.2",
    "react-custom-scroll": "^7.0.0",

    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-lazy-load-image-component": "^1.6.3",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-async-initial-state": "^0.3.0",
    "redux-logger": "^4.0.0",
    "redux-persist-webextension-storage": "1.0.2",
    "redux-thunk": "3.1.0",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@svgr/rollup": "^8.1.0",
    "@types/chrome": "0.0.308",
    "@types/node": "20.17.10",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@welldone-software/why-did-you-render": "^8.0.3",
    "autoprefixer": "^10.4.14",
    "clean-publish": "^4.2.0",
    "jest": "^29.6.1",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "react-devtools": "^6.1.1",
    "sass": "^1.83.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "manifest": {
    "host_permissions": ["https://*/*"],
    "permissions": ["tabs", "storage"],
    "content_security_policy": {
      "extension_pages": "script-src 'self' http://localhost:8097; object-src 'self'"
    }
  },
  "nodemonConfig": {
    "ignore": ["test/*", "docs/*"],
    "delay": 1000
  }
}
