    },
    "scripts": {
        "build": "node utils/production.js",
        "start": "node utils/webserver.js",
        "firefoxd": "SET Client=browser & npm run start",
        "chromed": "npm run start --env.CLIENT=chrome",
        "firefoxp": "SET NODE_ENV=production & SET Client=browser &  node utils/production.js",
        "chromep": "SET NODE_ENV=production & SET Client=chrome & node utils/production.js",
        "mount": "cd build & web-ext run --firefox=firefoxdeveloperedition",
        "pack": "cd build & web-ext build"
    },
