{
  "name": "excited-gem",
  "title": "Excited Gem",
  "version": "0.9.200",
  "description": "A tool to manage your tabs and other browser related features.",
  "homepage": "https://picwellwisher12pk.github.io/onetab-advanced/",
  "bugs": "{}",
  "logoimage": "images/logo.png",
  "license": "MIT",
  "author": "Amir Hameed",
  "client": {
    "chrome": "chrome",
    "firefox": "browser"
  },
  "browser": {
    "./dist/index.js": "./firefox/tabs.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:picwellwisher12pk/Excited-Gem.git"
  },
  "scripts": {
    "start": "nodemon",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "build": "node utils/production.js --mode=production",
    "postbuild": "git add . ",
    "firefoxd": "SET mode=development & npm run start",
    "chromed": "npm run start --env.CLIENT=chrome",
    "firefoxp": "SET NODE_ENV=production& SET BROWSER_CLIENT=firefox& node utils/production.js",
    "chromep": "SET NODE_ENV=production& SET BROWSER_CLIENT=chrome& node utils/production.js",
    "mount": "cd dist & web-ext run --firefox=firefoxdeveloperedition",
    "react-devtools": "react-devtools",
    "packf": "cd firefox & web-ext build",
    "test": "mocha './src/**/*.test.js' --require @babel/register",
    "depcheck": "npx depcheck",
    "outdated": "npm outdated",
    "lint-fix-all": "eslint './src/**/*.js' --fix --ext .js,.jsx,.ts,.tsx"
  },
  "quokka": {
    "babel": true,
    "react": true,
    "es6": true
  },
