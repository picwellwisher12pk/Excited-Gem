{
  "name": "excited-gem",
  "title": "Excited Gem",
  "version": "1.0.0",
  "description": "A tool to manage your tabs and other browser related features.",
  "homepage": "https://picwellwisher12pk.github.io/onetab-advanced/",
  "bugs": "{}",
  "logoimage": "images/logo.png",
  "license": "MIT",
  "author": "Amir Hameed",
  "client": {
    "chrome": "chrome",
    "firefox": "browser"
  },
  "browser": {
    "./dist/index.js": "./firefox/tabs.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:picwellwisher12pk/Excited-Gem.git"
  },
  "scripts": {
    "start": "nodemon",
    "dev": "run-p dev:*",
    "dev:clear":"npm run clear",
    "dev:prepare": "esno scripts/prepare.ts",
    "vite": "vite ",
    "dev:web": "vite --debug",
    "dev:js": "npm run build:js -- --mode development",
    "dev:background": "npm run build:background -- --mode development",
    "dev:popup": "npm run build:popup -- --mode development",
    "popup": "npm run build:popup -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:js build:background",
    "postbuild": "git add . ",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:js": "vite build --config vite.config.content.ts",
    "build:background": "vite build --config vite.config.background.ts",
    "build:popup": "vite build --config vite.config.popup.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension/* -o ./extension.zip",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf extension/dist extension/manifest.json extension.*",
    "lint": "eslint 'src/**/*.{json,ts,js,react}'",
    "react-devtools": "react-devtools",
    "depcheck": "npx depcheck",
    "outdated": "npm outdated",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@antfu/eslint-config": "^0.25.2",
    "@ffflorian/jszip-cli": "^3.1.6",
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
    "@iconify/json": "^2.1.80",
    "@reduxjs/toolkit": "^1.8.3",
    "@tailwindcss/forms": "^0.5.2",
    "@vitejs/plugin-react": "^2.0.0",
    "antd": "^4.21.7",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "cssnano": "^5.1.12",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "immutability-helper": "^3.1.1",
    "kolorist": "^1.5.1",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.0",
    "react-custom-scroll": "^4.4.0",
    "react-devtools": "^4.25.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.3.0",
    "react-lazy-load-image-component": "^1.5.5",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-async-initial-state": "^0.3.0",
    "redux-logger": "^3.0.6",
    "reduxed-chrome-storage": "^2.5.0",
    "rimraf": "^3.0.2",
    "supervisor": "^0.12.0",
    "unplugin-auto-import": "^0.9.4",
    "unplugin-icons": "^0.14.7",
    "vite": "^3.0.2",
    "vite-plugin-svgr": "^2.2.0",
    "vite-plugin-windicss": "^1.8.7",
    "web-ext": "^7.1.1",
    "webext-bridge": "^5.0.5",
    "webextension-polyfill": "^0.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-dynamic-import-vars": "latest",
    "@svgr/rollup": "^6.3.0",
    "autoprefixer": "^10.4.7",
    "clean-publish": "^4.0.1",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.14",
    "sass": "^1.53.0",
    "tailwindcss": "^3.1.6",
    "vite-plugin-chrome-extension": "^0.0.7",
    "web-ext-plugin": "^2.1.1"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": 1000
  }
}
